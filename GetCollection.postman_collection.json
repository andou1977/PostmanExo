		{
			"name": "Get Token",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Récupérer la réponse JSON\r",
							"const jsonData = pm.response.json();\r",
							"\r",
							"// Vérifier le status code\r",
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"// Vérifier que accessToken existe\r",
							"pm.test(\"Access token is present\", function () {\r",
							"    pm.expect(jsonData.accessToken).to.exist;\r",
							"});\r",
							"\r",
							"// Vérifier que refreshToken existe\r",
							"pm.test(\"Refresh token is present\", function () {\r",
							"    pm.expect(jsonData.refreshToken).to.exist;\r",
							"});\r",
							"\r",
							"// Stocker les tokens dans l'environnement\r",
							"pm.environment.set(\"accessToken\", jsonData.accessToken);\r",
							"pm.environment.set(\"refreshToken\", jsonData.refreshToken);\r",
							"\r",
							"//Vérifier que le token n’est pas vide\r",
							"\r",
							"pm.test(\"Access token is not empty\", function () {\r",
							"    pm.expect(jsonData.accessToken.length).to.be.above(10);\r",
							"});\r",
							"\r",
							"//Vérifier que le token ressemble à un JWT (3 parties séparées par des points)\r",
							"\r",
							"pm.test(\"Access token format is valid\", function () {\r",
							"    pm.expect(jsonData.accessToken.split('.').length).to.eql(3);\r",
							"});\r",
							"\r",
							"\r",
							"//Vérifier la présence d’un champ utilisateur (id, username…\r",
							"pm.test(\"User username is present\", function () {\r",
							"    pm.expect(jsonData.username).to.exist;\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
